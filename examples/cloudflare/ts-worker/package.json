{
  "name": "pname",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript Astro template",
  "main": "index.js",
  "scripts": {
    "build": "astro build",
    "dev": "NODE_ENV=development astro dev",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "xtest:clean": "rimraf ./transpiled/src ./transpiled/test",
    "xtest": "npm run test:clean && npm run transpile && mocha --require source-map-support/register --recursive transpiled/test",
    "transpile": "tsc --project ./test"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.0.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "astro": "file:../../../",
    "chai": "^4.2.0",
    "mocha": "^8.3.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  }
}
